doskey cat     = type $*
doskey clear   = cls
doskey cp      = copy $*
doskey kill    = taskkill /PID $*
doskey ls      = dir $*
doskey man     = help $*
doskey mv      = move $*
doskey ps      = tasklist $*
doskey pwd     = cd
doskey rm      = del $*
doskey rmr     = deltree $*
doskey cd~ = cd C:\Users\user_name
cls
